<?xml version="1.0"?>

<!-- Goto Dialog for the PureBasic IDE -->

<window id="#WINDOW_Goto" lang="Misc:GotoWindowTitle" label="Dialog_Goto" width="230">
  <vbox spacing="10">
    <string id="#GADGET_Goto_Line" />
    <hbox spacing="10" expand="equal">
      <button id="#GADGET_Goto_Ok"     lang="Misc:Ok" />
      <button id="#GADGET_Goto_Cancel" lang="Misc:Cancel" />
    </hbox>
  </vbox>

  <shortcut key="#PB_Shortcut_Return" id="#GADGET_Goto_Ok" />
  <shortcut key="#PB_Shortcut_Escape" id="#GADGET_Goto_Cancel" />
</window>